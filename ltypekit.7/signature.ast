TypeAtom        ::= ref
                  | "(" Type ")"
TypeList        ::= "[" TypeApplication "]"
TypeTuple       ::= "(" TypeApplcation ["," TypeApplication ]* ")"
TypeApplication ::= TypeAtom (TypeAtom)*
Type            ::= TypeApplication
                  | TypeApplication "=>" TypeApplication
                  | TypeApplication "->" TypeApplication
